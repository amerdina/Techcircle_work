package day_25MethodParamsAndMethodOverLoading;

public class tst2method {
 
	public static void main(String[] args) {
		
		 
			
		  System.out.println ( add2Num(1,3)) ;
			
			System.out.println(add3Num(1,2,3)+add2Num(1,3) );
			
			int result = add3Num(1,2,3) * 10;
			
			System.out.println(result);
			
			
			
			
//			
//			if (passwordValidation("assword@1")) {
//				System.out.println("Here is your new account.");
//				
//			}
//			
			
		}
		
		public static int add2Num(int num1, int num2) {
			return(num1+num2);
		}
		
		
		public static int add3Num(int num1, int num2,int num3) {
			//System.out.println(num1+num2);
			return num1 + num2 + num3;
		}
		
		
		
}
		
		
		
		
		
		
		
 
