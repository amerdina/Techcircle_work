package day27_classAndObject;

public class Dog {
	//instance Variable
	
	
    String breed;
	String size;
	int age;
	String color;
	
	
	public Dog() {}
	
	 public void printInfo() {
		   System.out.println("Name of the puppy is" + breed + "," + age + " yers old"+"size="+size+"color is " +color);
	   }
	   
	   public Dog(String breed,int age,String color,String size) {
		   
		this.age=age;
		this.breed=breed;
		this.size=size;
		this.color=color;
		   
		   
	   }
	   
	
	
