package day26_SaturdayReviewMethodWeek;

public class TasksMethod {

	public static void main(String[] args) {
		int  num1[]= {1,20,3,4};
		FindMax(num1);
		System.out.println(FindMax(num1)); 
	}
//		System.out.println(compainArray(a1,b));
		
		
	 
		
		
		
		public static int FindMax(int num1[]) {
			
	
		//int  num1[]= {1,20,3,4};
		int max=0;
			for(int dina:num1) {
	 		if(dina>max) {
	 			max =dina;
	 		}}
			return max ;
			 
		}
		
		

	} 

	
	 
	
	
	
	
	
	
	
	
 
