package library;

public class StringClassCode {

	public static void main(String[] args) {
/* returns the frequency of given char from the given string, as int
    accept one string, one character... return number of characters
    //String str = "hello";
     * char a = 'e';
     * output 1
 * */	
		String str="heeelloooo"	;
		char ch='l';
System.out.println("the number of letter "+  ch +" in "+ str+" is "+ FindNummOfChar(str,ch));
	
	}
	
	
 public static int FindNummOfChar(String str,char ch )	{ 
//String str="heeelloooo";
//char ch='o';
int count=0;
for (int i=0;i<str.length();i++) {
	char eachChar=str.charAt(i);
	if(ch==eachChar) {
		count++;
	}
}
return count ;
	
}}
